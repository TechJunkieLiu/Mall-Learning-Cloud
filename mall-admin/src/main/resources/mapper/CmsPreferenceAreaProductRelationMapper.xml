<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aiyangniu.admin.mapper.CmsPreferenceAreaProductRelationMapper">

    <resultMap id="BaseResultMap" type="com.aiyangniu.entity.model.pojo.cms.CmsPreferenceAreaProductRelation">
        <id column="id" jdbcType="BIGINT" property="id" />
        <result column="preference_area_id" jdbcType="BIGINT" property="preferenceAreaId" />
        <result column="product_id" jdbcType="BIGINT" property="productId" />
    </resultMap>

    <insert id="insertList">
        INSERT INTO cms_preference_area_product_relation (preference_area_id, product_id) VALUES
        <foreach collection="list" item="item" separator="," index="index">
            (#{item.preferenceAreaId, jdbcType=BIGINT},
            #{item.productId, jdbcType=BIGINT})
        </foreach>
    </insert>
</mapper>
